<% if current_user %>
  <div class="container text-center help">
    <h1>Dashboard</h1>
    <p id="notice"><%= notice %></p>
  </div>

<% else %>

  <div class="container text-center">
    <div class="emptydiv"></div>
    <h1>ClimbOn</h1>
    <span class="h1">Log In</span>
    <p id="alert"><%= alert %></p>
    <%= form_tag sessions_path do %>
      <div class="field">
        <%= text_field_tag :email, params[:email], placeholder: "Email" %>
      </div>
      <div class="field">
        <%= password_field_tag(:password, "", placeholder: "Password") %>
      </div>
      <br>
    <span class="actions"><%= submit_tag "Log In", :class => "btn btn-primary" %></span>
    <span class="actions"><%= link_to "Sign up", new_user_path, :class => "btn btn-primary" %></span>
    <% end %>
  </div>
<% end %>
